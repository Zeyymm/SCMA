{
  "name": "mental-health-app",
  "version": "1.0.0",
  "description": "SCMA - Smart Companion Mental Health App",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo build",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "doctor": "expo doctor",
    "install:clean": "rm -rf node_modules && npm install",
    "verify:sdk53": "node scripts/verify-sdk53.js",
    "setup:mobile": "npm run install:clean && npm run verify:sdk53"
  },
  "dependencies": {
    "expo": "~53.0.0",
    "expo-status-bar": "~2.0.0",
    "react": "18.3.1",
    "react-native": "0.76.3",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/stack": "^6.4.1",
    "react-native-screens": "~4.1.0",
    "react-native-safe-area-context": "~4.12.0",
    "@supabase/supabase-js": "^2.45.4",
    "react-native-url-polyfill": "^2.0.0",
    "@react-native-async-storage/async-storage": "~2.1.0",
    "victory-native": "^37.1.2",
    "react-native-youtube-iframe": "^2.3.0",
    "expo-image-picker": "~15.0.7",
    "expo-linking": "~7.0.3",
    "expo-notifications": "~0.29.9",
    "@google/generative-ai": "^0.21.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.0",
    "@types/react": "~18.3.12",
    "@types/react-native": "~0.76.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "typescript": "^5.6.0"
  },
  "keywords": [
    "mental-health",
    "react-native",
    "expo",
    "supabase",
    "healthcare",
    "malaysia",
    "crisis-intervention",
    "ai-chat",
    "emergency-calling"
  ],
  "author": "Azeem (Zeyymm)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zeyymm/SCMA.git"
  },
  "bugs": {
    "url": "https://github.com/Zeyymm/SCMA/issues"
  },
  "homepage": "https://github.com/Zeyymm/SCMA#readme",
  "private": true
}
